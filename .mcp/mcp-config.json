{
  "mcpServers": {
    "filesystem": {
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "d:/app/docker-operation"],
      "command": "npx",
      "disabled": false,
      "env": {}
    },
    "mcp-playwright": {
      "args": ["-y", "@playwright/mcp@latest"],
      "command": "npx",
      "disabled": false,
      "env": {}
    },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "command": "npx",
      "disabled": false,
      "env": {
        "MEMORY_FILE_PATH": "d:/app/docker-operation/.mcp/memory.json"
      }
    },
    "puppeteer": {
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"],
      "command": "npx",
      "disabled": false,
      "env": {}
    }
  }
}
